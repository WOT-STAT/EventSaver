{
  "name": "template",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "dev:docker": "docker compose -f dev/docker-compose.yml up --build",
    "build": "rimraf ./build && tsc",
    "prepare": "ts-patch install -s",
    "start:docker": "node index.js",
    "start:local": "cross-env NODE_ENV=local node build/index.js",
    "serve": "cross-env NODE_ENV=local nodemon",
    "serve:dev": "cross-env NODE_ENV=dev nodemon src/index.js",
    "migrate": "ts-node --esm src/db/migrate.ts",
    "rollback": "ts-node --esm src/db/rollback.ts",
    "generateJsonSchema": "npx ts-json-schema-generator --path ./src/types/events.ts --out ./src/types/types.json"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@clickhouse/client": "^0.2.8",
    "ajv": "^8.12.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "glob": "^8.1.0",
    "glob-promise": "^6.0.5",
    "jsonwebtoken": "^9.0.2",
    "knex": "^2.5.1",
    "redis": "^4.6.12",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/glob": "^8.1.0",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^18.19.8",
    "@types/uuid": "^9.0.7",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.22",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.2",
    "ts-patch": "^2.1.0",
    "tsconfig-paths": "^3.15.0",
    "typescript": "^4.9.5",
    "typescript-transform-paths": "^3.4.6"
  }
}
